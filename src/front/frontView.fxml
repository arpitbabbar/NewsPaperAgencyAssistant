<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="500.0" minWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="front.frontViewController">
   <children>
      <Label alignment="CENTER" prefHeight="49.0" prefWidth="500.0" text="NewsPaper Agency" underline="true">
         <font>
            <Font name="Times New Roman Bold" size="32.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="106.0" layoutY="49.0" prefHeight="40.0" prefWidth="288.0" text="Assistant" underline="true">
         <font>
            <Font name="Times New Roman Bold" size="32.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="155.0" layoutY="104.0" prefHeight="40.0" prefWidth="191.0" text="Login" underline="true">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Label>
      <ImageView fitHeight="156.0" fitWidth="159.0" layoutX="180.0" layoutY="153.0" onMouseClicked="#doLogin" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@login_icon.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="114.0" fitWidth="156.0" layoutX="180.0" layoutY="347.0" onMouseClicked="#doExit" pickOnBounds="true">
         <image>
            <Image url="@images.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
